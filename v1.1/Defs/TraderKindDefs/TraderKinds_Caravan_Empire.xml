<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Empire_Caravan_TraderGeneral</defName>
    <label>empire trader</label>
    <commonality>1</commonality>
    <faction>Empire</faction>
    <permitRequiredForTrading>TradeCaravan</permitRequiredForTrading>
    <stockGenerators>
      

      <!-- Silver -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>5000</countRange>
      </li>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>75</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentSpacer</thingDef>
        <countRange>25</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Plasteel</thingDef>
        <countRange>500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Uranium</thingDef>
        <countRange>500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Gold</thingDef>
        <countRange>500</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Synthread</thingDef>
        <countRange>250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Hyperweave</thingDef>
        <countRange>250</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineUltratech</thingDef>
        <countRange>50</countRange>
      </li>
      
      <!-- Drugs -->

      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>5</count>
            <chance>1</chance>
          </li>
        </countChances>
      </li>

      <!-- Weapons -->
      <li Class="StockGenerator_WeaponsRanged">
        <weaponTag>SpacerGun</weaponTag>
        <countRange>10</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>PsychicWeapon</tradeTag>
        <thingDefCountRange>3</thingDefCountRange>
      </li>
      <li Class="StockGenerator_WeaponsMelee">
        <weaponTag>UltratechMelee</weaponTag>
        <countRange>3</countRange>
      </li>
      <li Class="StockGenerator_WeaponsMelee">
        <weaponTag>Bladelink</weaponTag>
        <countRange>3</countRange>
      </li>

      <!-- Apparel -->
      <li Class="StockGenerator_Tag">
        <tradeTag>HiTechArmor</tradeTag>
        <thingDefCountRange>10</thingDefCountRange>
        <countRange>1</countRange>
      </li>
      <li Class="StockGenerator_Clothes">
        <apparelTag>Royal</apparelTag>
        <countRange>10</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>PsychicApparel</tradeTag>
        <thingDefCountRange>10</thingDefCountRange>
      </li>
  
      <!-- Implants -->
      <li Class="StockGenerator_Tag">
        <tradeTag>TechHediff</tradeTag>
        <thingDefCountRange>4</thingDefCountRange>
        <countRange>1</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>ImplantEmpireCommon</tradeTag>
        <thingDefCountRange>4</thingDefCountRange>
        <countRange>1</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>ImplantEmpireRoyal</tradeTag>
        <thingDefCountRange>4</thingDefCountRange>
        <countRange>1</countRange>
      </li>

      <!-- Buildings -->
      
      <!-- Exotic -->
      <!-- Exotic -->
      <li Class="StockGenerator_Tag">
        <tradeTag>Artifact</tradeTag>
        <thingDefCountRange>5</thingDefCountRange>
        <countRange>1</countRange>
      </li>
      <li Class="StockGenerator_Tag">
        <tradeTag>ExoticMisc</tradeTag>
        <thingDefCountRange>10</thingDefCountRange>
        <countRange>1</countRange>
        <excludedThingDefs>
          <li>Luciferium</li>
          <li>MedicineUltratech</li>
          <li>ComponentIndustrial</li>
          <li>Hyperweave</li>
          <li>ComponentSpacer</li>
        </excludedThingDefs>
      </li>
      
      <!-- Slaves -->

      <!-- Animals -->

      <!-- Buying Categories-->

      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>ImplantEmpireCommon</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>ImplantEmpireRoyal</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>HiTechArmor</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
      
    </stockGenerators>
  </TraderKindDef>

  <TraderKindDef>
    <defName>Empire_Caravan_TributeCollector</defName>
    <label>royal tribute collector</label>
    <requestable>False</requestable>
    <commonality>1</commonality>
    <category>TributeCollector</category>
    <tradeCurrency>Favor</tradeCurrency>
    <faction>Empire</faction>
    <hideThingsNotWillingToTrade>True</hideThingsNotWillingToTrade>
    <stockGenerators>
      <!-- Buying -->
      <li Class="StockGenerator_BuySlaves" />
      <li Class="StockGenerator_BuySingleDef">
        <thingDef>Gold</thingDef>
      </li>
    </stockGenerators>
  </TraderKindDef>

</Defs>
