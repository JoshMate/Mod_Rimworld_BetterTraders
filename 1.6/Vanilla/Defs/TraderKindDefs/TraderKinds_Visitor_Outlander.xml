<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <TraderKindDef>
    <defName>Visitor_Outlander_Standard</defName>
    <label>trader</label>
    <stockGenerators>
      
      <!-- Resources -->
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>500~1000</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>-1~10</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MealSurvivalPack</thingDef>
        <countRange>6~12</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineIndustrial</thingDef>
        <countRange>2~12</countRange>
      </li>
      <li Class="StockGenerator_ReinforcedBarrels">
        <countRange>1~4</countRange>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Cloth</li>
          <li>Chocolate</li>
        </thingDefs>
      </li>
      
      <!-- Drugs -->
      <li Class="StockGenerator_Category">
        <categoryDef>Drugs</categoryDef>
        <thingDefCountRange>-1~1</thingDefCountRange>
        <totalPriceRange>200~600</totalPriceRange>
      </li>

      <!-- Techprints -->
      <li Class="StockGenerator_Techprints">
        <countChances>
          <li>
            <count>1</count>
            <chance>0.5</chance>
          </li>
        </countChances>
      </li>

      <!-- Books -->
      <li Class="StockGenerator_Category">
        <categoryDef>Books</categoryDef>
        <excludedThingDefs>
          <li MayRequire="Ludeon.RimWorld.Anomaly">Tome</li> <!-- only available in Anomaly, handled seperately -->
        </excludedThingDefs>
        <thingDefCountRange>1</thingDefCountRange>
        <countRange>-1~1</countRange>
      </li>  
      
      <!-- Weapons -->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>WeaponRanged</tradeTag>
        <countRange>0~2</countRange>
      </li>

      <!-- Apparel -->
      <li Class="StockGenerator_MarketValue">
        <tradeTag>Clothing</tradeTag>
        <countRange>0~2</countRange>
      </li>

      <li Class="StockGenerator_MarketValue">
        <tradeTag>Armor</tradeTag>
        <countRange>0~2</countRange>
      </li>

      <!-- Implants -->
      <!-- none -->

      <!-- Buildings -->
      <!-- none -->
      
      <!-- Exotic -->
      <!-- none -->
      
      <!-- Slaves -->
      <!-- none -->
      
      <!-- Animals -->
      <!-- none -->
      
      <!-- Buying -->
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_BuyTradeTag"><tag>MusicalInstrument</tag></li>
      
    </stockGenerators>
  </TraderKindDef>

</Defs>
