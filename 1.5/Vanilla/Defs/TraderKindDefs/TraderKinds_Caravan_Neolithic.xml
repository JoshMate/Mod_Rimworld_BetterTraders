      <li Class="StockGenerator_Animals">
        <maxWildness>0.70</maxWildness>
        <tradeTagsSell>
          <li>AnimalFarm</li>
        </tradeTagsSell>
        <tradeTagsBuy>
          <li>AnimalExotic</li>
        </tradeTagsBuy>
        <kindCountRange>2~2</kindCountRange>
        <countRange>3~4</countRange>
        <createMatingPair>
          <li>AnimalFarm</li>
        </createMatingPair>
      </li>

